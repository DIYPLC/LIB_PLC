OB83_DATE_TIME  :DATE_AND_TIME; //Date and time OB83 started   
END_VAR

BEGIN
"DbDebug".OB83_CNT_ERR := "DbDebug".OB83_CNT_ERR + 1 ;//Счетчик вызовов обработчика ошибок OB83.
